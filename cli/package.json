		"zod": "^3.24.0",
		"type-fest": "^4.30.0",
		"fs-extra": "^11.2.0",
		"figlet": "^1.8.0",
		"simple-git": "^3.27.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^22.10.2",
		"@types/figlet": "^1.7.0",
		"nodemon": "^3.1.9",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
